  "compilerOptions": {
    "baseUrl": "./src/app",
    "outDir": "./dist/out-tsc",
    "paths": {
      "@core/*": [
        "./core/*"
      ],
      "@shared/*": [
        "./shared/*"
      ],
      "@bootstrap/*": [
        "./bootstrap/*"
      ],
      "@dashboard/*": [
        "./features/dashboard/*"
      ],
